cd ~/trac.mkaufmann.com.ar-rsync/
rpl -i -b "password_file = /home/humitos/apps/trachumitos/trac.htpasswd" "password_file = trac.htpasswd" conf/trac.ini
rpl -i -b "authz_file = /home/humitos/apps/trachumitos/conf/authzpolicy.conf" "authz_file = conf/authzpolicy.conf" conf/trac.ini
~/.virtualenvs/trac/bin/trac-admin . permission add anonymous TRAC_ADMIN
sleep 2 && firefox http://localhost:8080/trac.mkaufmann.com.ar-rsync &
~/.virtualenvs/trac/bin/tracd . -p 8080
